{
  "name": "Custom Web",
  "version": "0.1",
  "description": "Customize the web by adding your own js and css to websites.",
  "permissions": ["storage"],
  "background": {"scripts": ["background.js"]},
  "browser_action": {
    "default_icon": {
      "19": "icons/icon-19.png",
      "38": "icons/icon-38.png"
    },
    "default_title": "Customize this site"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "css": ["editor.css"],
    "js": ["vendor/jquery-2.0.3.min.js", "vendor/behave.js", "editor.js"]
  }],
  "icons": {
    "128": "icons/icon-128.png",
    "48": "icons/icon-48.png",
    "16": "icons/icon-16.png"
  },
  "options_page": "options.html",
  "web_accessible_resources": ["editor.html", "vendor/jquery-2.0.3.min.js"],
  "manifest_version": 2
}
